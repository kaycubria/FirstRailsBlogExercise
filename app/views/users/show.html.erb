<h2>Your Posts</h2>
<%@posts.each do |post|%>
  <h2><%=post.title%></h2>
  <h4><%=post.subhead%></h4>
  <p><%=post.content%></p>
  <p>*****</p>
	<form class="delete" action=<%="/users/#{@user.id}/posts/#{post.id}"%> method="post">
      <input type="hidden" name="_method" value="delete">
      <input type="submit" value="Delete Post">
    </form>
<%end%>
